{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=./mydb.db"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      },
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "../certs/aspnetapp.crt",
          "KeyPath": "../certs/aspnetapp.key"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
